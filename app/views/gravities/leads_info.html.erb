<div class="ibox-content">
  <input type="text" placeholder="Name of Form" class="form-control">
</div>
<div class="ibox-content">
  <input type="text" placeholder="<h1>Map your Fields</h1>" class="form-control">
</div>
<div class="ibox-content">
  <form role="form" id="form">
    <div class="form-group"><label>First Name</label> <input type="email" placeholder="First Name" class="form-control" required></div>
    <div class="form-group"><label>Last Name</label> <input type="password" placeholder="Last Name" class="form-control" name="lastname"></div>
    <div class="form-group"><label>Procedure of Interest</label> <input type="text" placeholder="Procedure of Interest" class="form-control" name="url"></div>
    <div class="form-group"><label>Email</label> <input type="text" placeholder="Email" class="form-control" name="number"></div>
    <div class="form-group"><label>Phone</label> <input type="text" placeholder="Phone" class="form-control" name="min"></div>
    <div class="form-group"><label>Comments</label> <input type="text" placeholder="Comments" class="form-control" name="max"></div>
    <div class="form-group"><label>Doctor Requested</label> <input type="text" placeholder="Doctor Requested" class="form-control" name="max"></div>
    <div class="form-group"><label>Consultation Date</label> <input type="text" placeholder="Consultation Date" class="form-control" name="max"></div>
    <div class="form-group"><label>Surgery Date</label> <input type="text" placeholder="Surgery Date" class="form-control" name="max"></div>
    <br>
    <button type="button" class="btn btn-white" data-dismiss="modal">Cancel</button>
    <button type="button" class="btn btn-primary"><%=  link_to "Finish",  dashboards_dashboard_1_path %></button>
  </form>
</div>